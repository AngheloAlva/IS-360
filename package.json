{
  "name": "otc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "pnpm dotenv -e .env.local -- prisma migrate dev",
    "db:push": "pnpm dotenv -e .env.local -- prisma db push",
    "db:seed": "pnpm dotenv -e .env.local -- ts-node ./prisma/seed/seed.ts",
    "prisma:generate": "pnpm dotenv -e .env.local -- prisma generate",
    "prisma:studio": "pnpm dotenv -e .env.local -- prisma studio",
    "production:build": "pnpm prisma generate && pnpm prisma migrate deploy && next build"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.2",
    "@prisma/client": "^6.4.1",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "better-auth": "^1.1.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv-cli": "^8.0.0",
    "eslint-config-prettier": "^10.0.2",
    "lucide-react": "^0.476.0",
    "next": "15.2.0",
    "next-themes": "^0.4.4",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "prisma": "^6.4.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "^5"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "sharp"
    ]
  }
}
